
<!-- Modal -->
<div class="modal fade" id="myModal" data-backdrop="static" data-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">Registro de Producto</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group">
                        <label for="Nombre">Nombre Producto:</label>
                        <input type="text" class="form-control form-control-sm" id="Nombre" placeholder="Ejemplo: Nintento Wii" autocomplete="off">
                    </div>
                    <div class="form-group">
                        <label for="Categoria">Categoria</label>
                        <input type="password" class="form-control" id="exampleInputPassword1">
                    </div>
                    <div class="form-group">
                        <label for="Precio">Precio:</label>
                        <input type="text" class="form-control form-control-sm" id="Precio" placeholder="Ejemplo: 40.00" autocomplete="off">
                    </div>
                    <div class="form-group">
                        <label for="Descuento">Descuento</label>
                        <input type="text" class="form-control form-control-sm" id="Descuento" placeholder="Ejemplo: 15" autocomplete="off">
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Understood</button>
            </div>
        </div>
    </div>
</div>

